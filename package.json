    "fetch": "^1.1.0",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "pg": "^8.11.3",
    "puppeteer": "^21.0.3",
    "rollup": "^3.29.1",
    "rollup-plugin": "^1.0.0",
